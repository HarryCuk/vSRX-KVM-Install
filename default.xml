<!--Configuration for the 'default' network configuration used for management-->
<network>
 <name>default</name>
 <forward mode='nat'/>
<nat>
 <port start ='1024' end='65535' />
 </nat>
<bridge name='virbr0' stp='on' delay='0' />
 <ip address='192.168.0.1' netmask='255.255.255.0'>
 <dhcp>
 <range start='192.168.0.11' end='192.168.0.50' />
 </dhcp>
 </ip>
</network>